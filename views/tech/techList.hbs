<div id="HIDDENDIV" class="container " >
    <h3 class="text-light badge  badge-info">Technical LIST</h3><br>
    <a href="/tech/add" class="badge  badge-primary" role="button">Add Technical page</a>

    <table class="table mt-4">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Name</th>
          <th scope="col">Thumbnail</th>
          <th scope="col">Update</th>
          <th scope="col">Delete</th>
        </tr>
      </thead>
      <tbody>
        {{#each Tech}}
        <tr>
          <th scope="row">{{@index}}</th>
          <td>{{ this.headName}}</td>
          <td>
            <img src="{{ this.pictureUrl1}}" alt="..." class="img-thumbnail" style="width: 70px;">
            
          </td>
          <td><a href="/tech/{{ this._id}}/eddit" class="btn btn-info" role="button">Eddit</a></td>
          <td>
            <form action="/tech/{{this._id}}?_method=DELETE" method="post" >
              <button class="btn btn-info" type="submit" onclick="return confirm('Are you sure you want to delete this product?')">Delete</button>
            </form> 
          </td>       
        </tr>
        {{/each}} 
      </tbody>
    </table>
  </div>
  